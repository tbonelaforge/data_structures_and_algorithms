class FractionType {
public:
    void Initialize(int numerator, int denominator) {
        this->numerator = numerator;
        this->denominator = denominator;
    }

    int getNumerator() {
        return numerator;
    }

    int getDenominator() {
        return denominator;
    }

private:
    int numerator;
    int denominator;
};
